@model RascalChat.Models.ViewUser
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title></title>
</head>
<body>
    @if (Model.Error != null)
    {
        <p>@Model.Error</p>
    }
    <form action="register" method="post">
        <label for="login">Username</label>
        <input type="text" name="login">
